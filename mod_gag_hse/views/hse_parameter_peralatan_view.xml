<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!--    ini tampilan tabel depan-->
    <record id="gag_oa_hse_parameter_peralatan_tree" model="ir.ui.view">
        <field name="name">gag.oa.hse.parameter.peralatan.tree</field>
        <field name="model">gag.oa.hse.parameter.peralatan</field>
        <field name="arch" type="xml">
            <tree string="Pemeriksaan" editable="bottom">
                <field name="no"/>
                <field name="pemeriksaan"/>
                <field name="kode"/>
                <field name="create_date"/>
                <field name="create_uid"/>
            </tree>
        </field>
    </record>
    <record id="gag_oa_hse_parameter_peralatan_action" model="ir.actions.act_window">
        <field name="name">Pemeriksaan</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">gag.oa.hse.parameter.peralatan</field>
        <field name="view_mode">tree</field>
        <field name="context">{}</field>
        <field name="domain"></field>
        <field name="help" type="html">
            <p class="o_view_noncontent_smiling_face">
                Submit your parameter here !
            </p>
        </field>
    </record>
</odoo>