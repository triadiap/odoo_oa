<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="ptgn_md_risk_level_action" model="ir.actions.act_window">
        <field name="name">PTGN MD Risk Level</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">ptgn.md.risk.level</field>
        <field name="view_mode">tree,form</field>
        <field name="target">current</field>
        <field name="context">{}</field>
        <field name="domain">[]</field>
        <field name="help" type="html">
            <p class="o_view_noncontent_smiling_face">
                PTGN Master Data - Risk Level
            </p>
        </field>
    </record>

    <record id="ptgn_md_risk_level_tree" model="ir.ui.view">
        <field name="name">ptgn.md.risk.level.tree</field>
        <field name="model">ptgn.md.risk.level</field>
        <field name="arch" type="xml">
            <tree>
                <field name="risk_level"/>
                <field name="kuadran"/>
            </tree>
        </field>
    </record>

    <record id="ptgn_md_risk_level_form" model="ir.ui.view">
        <field name="name">ptgn.md.risk.level.form</field>
        <field name="model">ptgn.md.risk.level</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <group>
                        <field name="risk_level"/>
                        <field name="kuadran"/>
                    </group>
                </group>
            </form>
        </field>
    </record>
</odoo>