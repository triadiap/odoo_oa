<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="mo_master_lokasi_action" model="ir.actions.act_window">
        <field name="name">MO Master Data Lokasi</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">mo.master.lokasi</field>
        <field name="view_mode">tree,form</field>
        <field name="target">current</field>
        <field name="context">{}</field>
        <field name="domain">[]</field>
        <field name="help" type="html">
            <p class="o_view_noncontent_smiling_face">
                PTGN MO and Shipping - Master Data Lokasi
            </p>
        </field>
    </record>

    <record id="mo_master_lokasi_tree" model="ir.ui.view">
        <field name="name">mo.master.lokasi.tree</field>
        <field name="model">mo.master.lokasi</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name"/>
            </tree>
        </field>
    </record>
</odoo>