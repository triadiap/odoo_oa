    <record id="gag_oa_scm_monitoring_nomor_lelang_tree" model="ir.ui.view">
        <field name="name">gag.oa.scm.monitoring.nomor.lelang.tree</field>
        <field name="model">gag.oa.scm.monitoring.nomor.lelang</field>
        <field name="arch" type="xml">
            <tree string="List Nomor Lelang" editable="bottom">
                <field name="bantex"/>
                <field name="nomor"/>
                <field name="nomor_lanjutan"/>
                <field name="keterangan"/>
                <field name="currency_id"/>
                <field name="harga"/>
            </tree>
        </field>
    </record>
